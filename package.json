{
  "name": "leozera-ui",
  "version": "0.3.6",
  "description": "My experimental React UI library",
  "repository": "git@github.com:leonardofaria/leozera-ui.git",
  "author": "Leonardo Faria <leonardofaria@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o docs",
    "deploy:github": "gh-pages -d docs",
    "deploy": "yarn storybook:build && yarn deploy:github",
    "test": "jest",
    "test:ci": "jest && codecov",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "lint:js": "eslint lib",
    "build:lib": "yarn build:js && yarn build:webpack && yarn build:css && yarn build:copy-files",
    "build:webpack": "NODE_ENV=production webpack",
    "build:js": "cross-env NODE_ENV=production babel lib/components -d dist --ignore '**/*.spec.js,**/*.stories.js'",
    "build:css": "node scripts/buildSass.js",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "publish": "yarn version && yarn build:lib && npm publish dist",
    "postpublish": "git push origin master --tags"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "@storybook/addon-centered": "^3.3.11",
    "@storybook/addon-info": "^3.3.11",
    "@storybook/addon-knobs": "^3.3.11",
    "@storybook/addon-options": "^3.3.11",
    "@storybook/addons": "^3.3.11",
    "@storybook/react": "^3.3.11",
    "autoprefixer": "^7.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-docgen": "^1.8.1",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "resolve-url-loader": "^2.2.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0"
  }
}
